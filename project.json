{
  "name": "nihildbuser",
  "root": "user",
  "projectType": "library",
  "implicitDependencies": ["nihildbshared"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "user"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "user"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf prisma/generated node_modules package-lock.json",
        "cwd": "user"
      }
    },
    "prisma:db:seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:db:seed",
        "cwd": "user"
      }
    },
    "prisma:db:push": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:db:push",
        "cwd": "user"
      }
    },
    "prisma:migrate:reset": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:migrate:reset",
        "cwd": "user"
      }
    },
    "prisma:migrate:reset:force": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:migrate:reset:force",
        "cwd": "user"
      }
    },
    "publish:package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run publish:package",
        "cwd": "user",
        "env": { "NODE_ENV": "prod" }
      },
      "dependsOn": ["build"]
    }
  }
}
